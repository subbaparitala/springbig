<h1 class="text-center"> Import CSV </h1>

<%= form_tag identifiers_path, multipart: true, method: :post do %>
  <div class="form-group row">
    <%= label_tag :name, 'Identifier Name', class: 'col-sm-2 col-form-label'%>
    <%= text_field_tag :name, '', class: 'form-control col-sm-10' %>
  </div>
  <div class="form-group row">
    <%= label_tag :file, 'Browse file', class: 'col-sm-2 col-form-label' %>
    <%= file_field_tag :file%>
  </div>
  <%= submit_tag "Upload", class: 'btn btn-primary' %>
<% end %>